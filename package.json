  "dependencies": {
    "@babel/runtime": "^7.20.7",
    "babel-plugin-module-resolver": "^5.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongodb": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
